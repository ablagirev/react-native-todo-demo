{
  "env": { "browser": true, "es6": true },
  "extends": ["airbnb-base", "plugin:react/recommended", "plugin:import", "plugin:prettier/recommended"],
  "parserOptions": { "ecmaVersion": 6 },
  "parser": "babel-eslint",
  "rules":{
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "indent": [2, "tab"],
    "no-tabs": 0,
    "no-use-before-define": ["error", { "variables": false }],
    "react/prop-types": 0,
    "semi": ["error", "never"],
    "global-require": 0,
    "quotes": ["error", "single"],
    "no-multi-spaces": ["error"],
    "prettier/prettier": "error"
  }
}